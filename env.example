# Configuration Notion MCP
# Copiez ce fichier vers .env et remplissez les valeurs

# Token d'intégration Notion (obtenu depuis developers.notion.com)
NOTION_TOKEN=secret_your_notion_integration_token_here

# Configuration OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Configuration LangGraph Studio (optionnel)
LANGGRAPH_API_KEY=your_langgraph_api_key_here

# Configuration des agents
# Niveau de raisonnement OpenAI (minimal, low, medium, high)
REASONING_EFFORT=minimal

# Verbosité des réponses (low, medium, high)
VERBOSITY=low

# Limite de tokens de sortie
MAX_OUTPUT_TOKENS=300

# Mode debug
DEBUG=false
